.search-text-view {  /* 包含搜索框的view */
  position: absolute;
  width: 80%;
  left: 10%;
}

.search-text {    /*  搜索框使用的class */
  border-radius: 25px;
  font-size: 12px;
  background-color: rgba(255, 255, 255, 0.2);
  margin-top: 5%;
}

.image-search {    /* 轮播图swiper组件的class */
  position: absolute;
  width: 100%;
  height: 30%;
  z-index: -1;
}
.image{
  width: 100%;
}